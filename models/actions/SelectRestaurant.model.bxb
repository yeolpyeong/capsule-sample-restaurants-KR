action (SelectRestaurant) {
  type(Search)
  collect {
    input (candidate) {
      type (CandidateRestaurant)
      min (Required) max (One)
      prompt-behavior (AlwaysSelection)
    }
  }
  output (SelectedRestaurant){
    evaluate{
      SelectedRestaurant{
        candidate: $expr(candidate)
      }
    }
  }
}
